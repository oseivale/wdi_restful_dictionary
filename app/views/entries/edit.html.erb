<p>This is the edit page! <%= params[:id] %></p>

<%= link_to 'Edit Entry', edit_entry_path %>

<%= form_for @entry do |f| %>

<p>
  <%= f.label :word %>
  <%= f.text_field :word %>
</p>

<p>
  <%= f.label :language %>
  <%= f.text_field :language %>
</p>

<p>
  <%= f.label :definition %>
  <%= f.text_field :definition %>
</p>

<%= f.submit %>

<% end %>
