<p>The new entry form will go here later!</p>

<%= link_to 'Back to Entries', entries_url %>

<%= form_for @entry do |f| %>

  <p>
    <%= f.label :word %>
    <%= f.text_field :word %>
  </p>

  <p>
    <%= f.label :language %>
    <%= f.text_field :language %>
  </p>

  <p>
    <%= f.label :definition %>
    <%= f.text_area :definition %>
  </p>

  <%= f.submit %>

<% end %>
